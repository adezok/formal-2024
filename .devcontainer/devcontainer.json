{
	"name": "LEAN development environment",
	"dockerFile": "Dockerfile",
	"remoteUser": "dev",
	"postCreateCommand": "git pull && lake exe cache get",
	"customizations": {
		"vscode": {
			"extensions": [
				"leanprover.lean4",
				"ms-azuretools.vscode-docker",
				"paperproof.paperproof"
			]
		}
	},
	"containerEnv": {
		"SHELL": "/bin/bash"
	}
}